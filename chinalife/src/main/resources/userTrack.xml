<?xml version="1.0" encoding="UTF-8"?> 
<workbook> 
   <worksheet name="Sheet1"> 
    <section startRow="0" endRow="0"/> 
    <loop startRow="1" endRow="1" items="users"     var="user"     varType="com.riil.lbs.entity.User"> 
    <section startRow="1" endRow="1"> 
    <mapping row="1" col="0">user.usercode</mapping> 
    <mapping row="1" col="1">user.username</mapping> 
    <mapping row="1" col="2">user.college</mapping> 
    <mapping row="1" col="3">user.classes</mapping> 
    <mapping row="1" col="4">user.usermac</mapping> 
    <mapping row="1" col="5">user.usertel</mapping> 
    <mapping row="1" col="6">user.email</mapping> 
   
   </section> 
    <loopbreakcondition> 
     <rowcheck offset="0"> 
        <cellcheck offset="0"></cellcheck> 
      </rowcheck> 
    </loopbreakcondition> 
    </loop> 
    </worksheet> 
</workbook> 

